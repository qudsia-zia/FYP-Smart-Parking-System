<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hyperparkfyp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Hyper Park"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HyperParkFYP">
        <activity
            android:name=".ResetPassword"
            android:label="Reset your Account Password"
            android:exported="true" />
        <activity
            android:name=".contact"
            android:label="Contact the App Developer"
            android:exported="true" />
        <activity
            android:name=".reservation"
            android:label="Reserve your parking spot"
            android:exported="true" />
        <activity android:name=".Home" />
        <activity
            android:name=".Login"
            android:label="Please Login First" />
        <activity
            android:name=".SelectMall"
            android:label="Select Your Mall.." />
        <activity
            android:name=".SelectCity"
            android:label="Select Your City.." />
        <activity
            android:name=".ParkingSlots"
            android:label="Parking Slots.." />
        <activity
            android:name=".Register"
            android:label="Register New User.." />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>